<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.cozy">

    <uses-permission android:name="android.permission.INTERNET" />

    <application
        android:name=".GlobalApplication"
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".onboarding.OnBoardingPreferenceActivity"></activity>
        <activity android:name=".onboarding.OnBoardingActivity"/>
        <activity android:name=".views.main.RecommendDetailActivity"/>
        <activity android:name=".views.category.CategoryListActivity"/>
        <activity android:name=".views.mypage.LoginActivity"/>
        <activity android:name=".views.mypage.ProfileActivity"/>
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".views.main.event.EventActivity"/>
        <meta-data
            android:name="com.kakao.sdk.AppKey"
            android:value="@string/kakao_app_key" />
        <activity android:name=".views.mypage.NoInterestsActivity"/>
        <activity android:name=".views.main.event.EventDetailActivity" />
    </application>

</manifest>